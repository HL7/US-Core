                    {
                        "extension": [
                            {
                                "url": "http://hl7.org/fhir/StructureDefinition/elementdefinition-type-must-support",
                                "valueBoolean": true
                            }
                        ],
                        "code": "dateTime"
                    },
                    {
                        "code": "Period"
                    }
                ],
                "condition": [
                    "us-core-8"
                ],
                "mustSupport": true
            },
            {
                "id": "DiagnosticReport.issued",
                "path": "DiagnosticReport.issued",
                "short": "When the report was released",
                "condition": [
                    "us-core-9"
                ],
                "mustSupport": true
            },
            {
